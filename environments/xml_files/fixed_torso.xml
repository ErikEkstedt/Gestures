<mujoco model="fixed torso">
	<compiler angle="degree" inertiafromgeom="true"/>

	<default>
		<joint armature="1" damping="1" limited="true"/>
		<geom conaffinity="1" condim="3" friction="0.8 0.1 0.1" contype="1" margin="0.001" material="geom" rgba="0.8 0.6 .4 0.8"/>
		<motor ctrllimited="true" ctrlrange="-.4 .4"/>
	</default>

	<option integrator="RK4" iterations="50" solver="PGS" timestep="0.003">
		<!-- <flags solverstat="enable" energy="enable"/>-->
	</option>
	<size nkey="5" nuser_geom="1"/>
	<visual>
		<map fogend="5" fogstart="3"/>
	</visual>

	<worldbody>
		<!-- Robot -->
		<body name="robot" pos="0 0 0" >
			<geom fromto="-.02 -.07 0 -.02 .07 0" name="robot_pelvis" size="0.09" type="capsule" rgba="0 1 1 1"/>
			<joint armature="0.02" axis="1 0 0" damping="5" name="robot_abd_x" pos="0 0 -0.1" range="-25 25" stiffness="10" type="hinge"/>
			<joint armature="0.02" axis="0 1 0" damping="5" name="robot_abd_y" pos="0 0 -0.065" range="-25 30" stiffness="10" type="hinge"/>
			<joint armature="0.02" axis="0 0 1" damping="5" name="robot_abd_z" pos="0 0 -0.065" range="-25 25" stiffness="20" type="hinge"/>

			<!-- mid -->
			<body name="robot_mid" pos="-.01 0 0.140" quat="1.000 0 -0.002 0">
				<geom fromto="0 -.06 0 0 .06 0" name="robot_mid" size="0.06" type="capsule" rgba="1.0 0 0 1"/>
				<joint armature="0.02" axis="0 0 1" damping="5" name="robot_upper_abs" pos="0 0 0.015" range="-10 10" stiffness="20" type="hinge"/>
				
				<body name="robot_torso" pos="0 0 0.14" quat="1.000 0 -0.002 0">
					<geom fromto="0 -.07 0 0 .07 0" name="torso1" size="0.07" rgba="0 1 0 1" type="capsule"/>

					<!-- head -->
					<body name="head" pos="-.01 0 0.04" quat="1.000 0 -0.002 0">
						<joint armature="0.02" axis="1 0 0" damping="3" name="robot_neck_x" pos="0 0 0.01" range="-15 15" stiffness="3" type="hinge"/>
						<joint armature="0.02" axis="0 1 0" damping="3" name="robot_neck_y" pos="0 0 0.01" range="-15 15" stiffness="3" type="hinge"/>
						<joint armature="0.02" axis="0 0 1" damping="3" name="robot_neck_z" pos="0 0 0.01" range="-15 15" stiffness="3" type="hinge"/>
						<geom name="robot_head" pos="0 0 .17" size=".09" type="sphere" user="258"/>
					</body>

					<!-- R-Shoulder -->
					<body name="robot_right_shoulder" pos="0 -0.17 0.06">
						<geom contype="0" name="robot_right_shoulder" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
					</body>
					<!-- R-arm -->
					<body name="robot_right_upper_arm" pos="0 -0.17 0.06">
						<geom contype="0" name="robot_right_shoulder" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
						<joint armature="0.0068" axis="2 1 1" name="robot_right_shoulder1" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
						<joint armature="0.0051" axis="0 -1 1" name="robot_right_shoulder2" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
						<geom fromto="0 0 0 .16 -.16 -.16" name="robot_right_uarm1" size="0.04 0.16" type="capsule"/>

						<!-- Elbow -->
						<body name="robot_right_elbow" pos=".18 -.18 -.18">
							<geom contype="0" name="robot_right_elbow" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
						</body>

						<body name="robot_right_lower_arm" pos=".18 -.18 -.18">
							<joint armature="0.0028" axis="0 -1 1" name="robot_right_elbow" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
							<geom fromto="0.01 0.01 0.01 .17 .17 .17" name="robot_right_larm" size="0.031" type="capsule"/>

							<!-- Hand -->
							<body name="robot_right_hand" pos=".18 .18 .18">
								<geom contype="0" name="robot_right_hand" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
							</body>

						</body>
					</body>

					<!-- L-Shoulder -->
					<body name="robot_left_shoulder" pos="0 0.17 0.06">
						<geom contype="0" name="robot_left_shoulder" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
					</body>
					<!-- L-arm -->
					<body name="robot_left_upper_arm" pos="0 0.17 0.06">
						<joint armature="0.0068" axis="2 -1 1" name="robot_left_shoulder1" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
						<joint armature="0.0051" axis="0 1 1" name="robot_left_shoulder2" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
						<geom fromto="0 0 0 .16 .16 -.16" name="robot_left_uarm1" size="0.04 0.16" type="capsule"/>

						<!-- Elbow -->
						<body name="robot_left_elbow" pos=".18 .18 -.18">
							<geom contype="0" name="robot_left_elbow" pos="0 0 0" rgba="1.0 0.0 0.0 1" size=".03" type="sphere"/>
						</body>

						<body name="robot_left_lower_arm" pos=".18 .18 -.18">
							<joint armature="0.0028" axis="0 -1 -1" name="robot_left_elbow" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
							<geom fromto="0.01 -0.01 0.01 .17 -.17 .17" name="robot_left_larm" size="0.031" type="capsule"/>

							<!-- Hand -->
							<body name="robot_left_hand" pos=".18 -.18 .18">
								<geom contype="0" name="robot_left_hand" pos="0 0 0" rgba="0.9 0.0 0.9 1" size=".03" type="sphere"/>
							</body>
						</body>
					</body>
				</body>
			</body>
		</body>

		<!-- Target -->
		<body name="target0" pos="0.3 0.5 .2">
			<joint armature="0" axis="1 0 0" damping="0" limited="true" name="target0_x" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
			<joint armature="0" axis="0 1 0" damping="0" limited="true" name="target0_y" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
			<joint armature="0" axis="0 0 1" damping="0" limited="true" name="target0_z" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
			<geom conaffinity="0" contype="0" name="target0" pos="0 0 0" rgba="0 1.0 0 1" size=".05" type="sphere"/>
		</body>
    <!--  -->
		<!-- <!&#45;&#45; Target &#45;&#45;> -->
		<!-- <body name="target1" pos="0 0 .01"> -->
		<!-- 	<joint armature="0" axis="1 0 0" damping="0" limited="true" name="target1_x" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<joint armature="0" axis="0 1 0" damping="0" limited="true" name="target1_y" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<joint armature="0" axis="0 0 1" damping="0" limited="true" name="target1_z" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<geom conaffinity="0" contype="0" name="target1" pos="0 0 0" rgba="0.1 0.2 0.9 1" size=".05" type="sphere"/> -->
		<!-- </body> -->
    <!--  -->
		<!-- <!&#45;&#45; Target &#45;&#45;> -->
		<!-- <body name="target2" pos="0 0 .01"> -->
		<!-- 	<joint armature="0" axis="1 0 0" damping="0" limited="true" name="target2_x" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<joint armature="0" axis="0 1 0" damping="0" limited="true" name="target2_y" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<joint armature="0" axis="0 0 1" damping="0" limited="true" name="target2_z" pos="0 0 0" range="&#45;.27 .27" stiffness="0" type="slide"/> -->
		<!-- 	<geom conaffinity="0" contype="0" name="target2" pos="0 0 0" rgba="0.2 0.9 0.2 1" size=".05" type="sphere"/> -->
		<!-- </body> -->

	</worldbody>
	<actuator><!-- this section is not supported, same constants in code -->
		<motor gear= "100" joint= "robot_abd_y"           name = "robot_abd_y" />
		<motor gear= "100" joint= "robot_abd_z"           name = "robot_abd_z" />
		<motor gear= "100" joint= "robot_abd_x"           name = "robot_abd_x" />
		<motor gear= "100" joint= "robot_upper_abs"       name = "robot_upper_abs" />
		<motor gear= "25"  joint= "robot_neck_y"          name = "robot_neck_y" />
		<motor gear= "25"  joint= "robot_neck_z"          name = "robot_neck_z" />
		<motor gear= "25"  joint= "robot_neck_x"          name = "robot_neck_x" />
		<motor gear= "25"  joint= "robot_right_shoulder1" name = "robot_right_shoulder1 " />
		<motor gear= "25"  joint= "robot_right_shoulder2" name = "robot_right_shoulder2" />
		<motor gear= "25"  joint= "robot_right_elbow"     name = "robot_right_elbow" />
		<motor gear= "25"  joint= "robot_left_shoulder1"  name = "robot_left_shoulder1" />
		<motor gear= "25"  joint= "robot_left_shoulder2"  name = "robot_left_shoulder2" />
		<motor gear= "25"  joint= "robot_left_elbow"      name = "robot_left_elbow" />
	</actuator>
</mujoco>
