<<mujoco model="humanoid">
    <compiler angle="degree" inertiafromgeom="true"/>
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="1" condim="3" friction="0.8 0.1 0.1" contype="1" margin="0.001" material="geom" rgba="0.8 0.6 .4 1"/>
        <motor ctrllimited="true" ctrlrange="-.4 .4"/>
    </default>
    <option integrator="RK4" iterations="50" solver="PGS" timestep="0.003">
        <!-- <flags solverstat="enable" energy="enable"/>-->
    </option>
    <size nkey="5" nuser_geom="1"/>
    <visual>
        <map fogend="5" fogstart="3"/>
    </visual>
    <worldbody>
			<!-- Arena -->
			<!-- <geom conaffinity="0" contype="0" name="ground" pos="0 0 0" rgba="0.9 0.1 0.0 1" size="1 1 10" type="plane"/> -->
			<!-- <body name="robot" pos="0 0 0.4"> -->
			<geom fromto="0 -.07 0 0 .07 0" name="robot_torso1" size="0.07" type="capsule"/>
			<geom name="robot_head" pos="0 0 .19" size=".09" type="sphere" user="258"/>
			<geom fromto="-.01 -.06 -.12 -.01 .06 -.12" name="robot_uwaist" size="0.06" type="capsule"/>

				<body name="robot" pos="-.01 0 -0.260" quat="1.000 0 -0.002 0">
					<geom fromto="0 -.06 0 0 .06 0" name="robot_lwaist" size="0.06" type="capsule"/>
					<joint armature="0.02" axis="0 0 1" damping="5" name="robot_abdomen_z" pos="0 0 0.065" range="-45 45" stiffness="20" type="hinge"/>
					<joint armature="0.02" axis="0 1 0" damping="5" name="robot_abdomen_y" pos="0 0 0.065" range="-75 30" stiffness="10" type="hinge"/>
					<body name="robot_pelvis" pos="0 0 -0.165" quat="1.000 0 -0.002 0">
						<joint armature="0.02" axis="1 0 0" damping="5" name="robot_abdomen_x" pos="0 0 0.1" range="-35 35" stiffness="10" type="hinge"/>
						<geom fromto="-.02 -.07 0 -.02 .07 0" name="robot_butt" size="0.09" type="capsule"/>

						<!-- LEGS -->
						<body name="robot_right_thigh" pos="0 -0.1 -0.04">
							<joint armature="0.01" axis="1 0 0" damping="5" name="robot_right_hip_x" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
							<joint armature="0.01" axis="0 0 1" damping="5" name="robot_right_hip_z" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
							<joint armature="0.01" axis="0 1 0" damping="5" name="robot_right_hip_y" pos="0 0 0" range="-120 20" stiffness="20" type="hinge"/>
							<geom fromto="0 0 0 0 0.01 -.34" name="robot_right_thigh1" size="0.06" type="capsule"/>
							<body name="robot_right_shin" pos="0 0.01 -0.403">
								<joint armature="0.0060" axis="0 -1 0" name="robot_right_knee" pos="0 0 .02" range="-160 -2" stiffness="1" type="hinge"/>
								<geom fromto="0 0 0 0 0 -.3" name="robot_right_shin1" size="0.049" type="capsule"/>
								<body name="robot_right_foot" pos="0 0 -0.45">
										<geom name="robot_right_foot" pos="0 0 0.1" size="0.075" type="sphere" user="0"/>
									</body>
							</body>
						</body>
						<body name="robot_left_thigh" pos="0 0.1 -0.04">
							<joint armature="0.01" axis="-1 0 0" damping="5" name="robot_left_hip_x" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
							<joint armature="0.01" axis="0 0 -1" damping="5" name="robot_left_hip_z" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
							<joint armature="0.01" axis="0 1 0" damping="5" name="robot_left_hip_y" pos="0 0 0" range="-120 20" stiffness="20" type="hinge"/>
							<geom fromto="0 0 0 0 -0.01 -.34" name="robot_left_thigh1" size="0.06" type="capsule"/>
							<body name="robot_left_shin" pos="0 -0.01 -0.403">
								<joint armature="0.0060" axis="0 -1 0" name="robot_left_knee" pos="0 0 .02" range="-160 -2" stiffness="1" type="hinge"/>
								<geom fromto="0 0 0 0 0 -.3" name="robot_left_shin1" size="0.049" type="capsule"/>
								<body name="robot_left_foot" pos="0 0 -0.45">
										<geom name="robot_left_foot" type="sphere" size="0.075" pos="0 0 0.1" user="0" />
								</body>
							</body>
						</body>
					</body>
				</body>
				<body name="robot_right_upper_arm" pos="0 -0.17 0.06">
					<joint armature="0.0068" axis="2 1 1" name="robot_right_shoulder1" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
					<joint armature="0.0051" axis="0 -1 1" name="robot_right_shoulder2" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
					<geom fromto="0 0 0 .16 -.16 -.16" name="robot_right_uarm1" size="0.04 0.16" type="capsule"/>
					<body name="robot_right_lower_arm" pos=".18 -.18 -.18">
						<joint armature="0.0028" axis="0 -1 1" name="robot_right_elbow" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
						<geom fromto="0.01 0.01 0.01 .17 .17 .17" name="robot_right_larm" size="0.031" type="capsule"/>
						<body name="robot_right_hand" pos=".17 .17 .17">
							<geom name="robot_right_hand" pos=".0 .0 .0" size="0.04" type="sphere" rgba="1 0 1 1"/>
						</body>
					</body>
				</body>
				<body name="robot_left_upper_arm" pos="0 0.17 0.06">
					<joint armature="0.0068" axis="2 -1 1" name="robot_left_shoulder1" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
					<joint armature="0.0051" axis="0 1 1" name="robot_left_shoulder2" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
					<geom fromto="0 0 0 .16 .16 -.16" name="robot_left_uarm1" size="0.04 0.16" type="capsule"/>
					<body name="robot_left_lower_arm" pos=".18 .18 -.18">
						<joint armature="0.0028" axis="0 -1 -1" name="robot_left_elbow" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
						<geom fromto="0.01 -0.01 0.01 .17 -.17 .17" name="robot_left_larm" size="0.031" type="capsule"/>
						<body name="robot_left_hand" pos=".17 -.17 .17">
								<geom name="robot_left_hand" pos=".0 .0 .0" size="0.04" type="sphere" rgba="1 0 1 1"/>
						</body>
					</body>
				</body>
        <!-- </body> -->
				<!-- Target -->
			<body name="target" pos="0.3 0.3 .01">
				<joint armature="0" axis="1 0 0" damping="0" limited="true" name="target_x" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
				<joint armature="0" axis="0 1 0" damping="0" limited="true" name="target_y" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
				<joint armature="0" axis="0 0 1" damping="0" limited="true" name="target_z" pos="0 0 0" range="-.27 .27" stiffness="0" type="slide"/>
				<geom conaffinity="0" contype="0" name="target" pos="0 0 0" rgba="0.9 0.2 0.2 1" size=".025" type="sphere" mass="0"/>
			</body>
    </worldbody>
    <tendon>
        <fixed name="robot_left_hipknee">
            <joint coef="-1" joint="left_hip_y"/>
            <joint coef="1" joint="left_knee"/>
        </fixed>
        <fixed name="robot_right_hipknee">
            <joint coef="-1" joint="right_hip_y"/>
            <joint coef="1" joint="right_knee"/>
        </fixed>
    </tendon>
    <actuator><!-- this section is not supported, same constants in code -->
        <motor gear="100" joint="robot_abdomen_y"      name ="robot_abdomen_y"/>
        <motor gear="100" joint="robot_abdomen_z"      name ="robot_abdomen_z"/>
        <motor gear="100" joint="robot_abdomen_x"      name ="robot_abdomen_x"/>
        <motor gear="100" joint="robot_right_hip_x"    name ="robot_right_hip_x"/>
        <motor gear="100" joint="robot_right_hip_z"    name ="robot_right_hip_z"/>
        <motor gear="300" joint="robot_right_hip_y"    name ="robot_right_hip_y"/>
        <motor gear="200" joint="robot_right_knee"     name ="robot_right_knee"/>
        <motor gear="100" joint="robot_left_hip_x"     name ="robot_left_hip_x"/>
        <motor gear="100" joint="robot_left_hip_z"     name ="robot_left_hip_z"/>
        <motor gear="300" joint="robot_left_hip_y"     name ="robot_left_hip_y"/>
        <motor gear="200" joint="robot_left_knee"      name ="robot_left_knee"/>
        <motor gear="25" joint="robot_right_shoulder1" name ="robot_right_shoulder1"/>
        <motor gear="25" joint="robot_right_shoulder2" name ="robot_right_shoulder2"/>
        <motor gear="25" joint="robot_right_elbow"     name ="robot_right_elbow"/>
        <motor gear="25" joint="robot_left_shoulder1"  name ="robot_left_shoulder1"/>
        <motor gear="25" joint="robot_left_shoulder2"  name ="robot_left_shoulder2"/>
        <motor gear="25" joint="robot_left_elbow"      name ="robot_left_elbow"/>
    </actuator>
